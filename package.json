		"factorial",
		"app"
	],
	"author": "Julian R Villabona A",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/JrVillabona/qa-tribalscale/issues"
	},
	"homepage": "https://github.com/JrVillabona/qa-tribalscale#readme",
	"devDependencies": {
		"cypress": "^4.8.0",
		"cypress-cucumber-preprocessor": "^2.5.0"
	},
	"cypress-cucumber-preprocessor": {
		"nonGlobalStepDefinitions": true
	}
